<script>
$(function(){

    function share_off() {
        $(".sns_share").removeAttr('click');
        $(".sns_share").removeClass('sns_share_add');
        $(".lsns_share").remove();
    }

    function share_sns(lsharer) {
        var winHeight = 500;
        var winWidth = 550;
        var winTop = (screen.height / 2) - (winHeight / 2);
        var winLeft = (screen.width / 2) - (winWidth / 2);
        window.open(lsharer, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width='+winWidth+',height='+winHeight);
    }

    $(".sns_share").click(function() {
        if (!$(this).attr("click")) {
            $(this).attr('click', 1);
            $(this).addClass('sns_share_add');
            $(".bsns_share").append($("#lsns_share").html());
        }
        else {
            share_off();
        }
    });

    $(document).on('click', '.iconsns', function() {
        var post_link = $('link[rel="canonical"]').attr("href");
        var post_title = $('title').text();

        if ($(this).hasClass("iconsns_fb")) {
            var lsharer = "https://www.facebook.com/sharer/sharer.php?u=" + post_link;
            share_sns(lsharer);
        }
        else if ($(this).hasClass("iconsns_fbmsg")) {
            var lsharer = "https://www.facebook.com/dialog/send?app_id=1450466948435476&display=popup&link="+post_link+"&redirect_uri="+post_link;
            share_sns(lsharer);
        }
        else if ($(this).hasClass("iconsns_zalo")) {
            alert("Chức năng đang phát triển");
        }
        else if ($(this).hasClass("iconsns_twitter")) {
            var lsharer = "https://twitter.com/intent/tweet?&url="+ post_link;
            share_sns(lsharer);
        }
        else if ($(this).hasClass("iconsns_tele")) {
            var lsharer = "https://t.me/share/url?url="+ post_link;
            share_sns(lsharer);
        }
        else if ($(this).hasClass("iconsns_skype")) {
            var lsharer = "https://web.skype.com/share?url="+ post_link;
            share_sns(lsharer);
        }
        if (!$(this).hasClass("iconsns_zalo")) {
            share_off();
        }
        
    });
});
</script>
